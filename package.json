{
  "name": "passman",
  "version": "1.0.1",
  "description": "A simple password manager and generator",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "passman": "lib/cli.js"
  },
  "files": [
    "src",
    "lib",
    "!src/__tests__"
  ],
  "scripts": {
    "lint": "eslint src",
    "format": "prettier --write src",
    "build": "rm -rf lib && tsc",
    "test": "jest --coverage",
    "coveralls": "coveralls < coverage/lcov.info",
    "prepare": "npm run build",
    "docs": "typedoc src/index.ts",
    "pages": "npm run docs && gh-pages -d docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/d-band/passman.git"
  },
  "keywords": [
    "password",
    "generator",
    "manager",
    "master",
    "AES",
    "SHA256",
    "pbkdf2",
    "seed"
  ],
  "author": "d-band",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/d-band/passman/issues"
  },
  "homepage": "https://github.com/d-band/passman#readme",
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.34",
    "@types/prompts": "^2.0.9",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "coveralls": "^3.1.0",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "gh-pages": "^3.1.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.3",
    "typedoc": "^0.20.30",
    "typescript": "^4.2.2"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "commander": "^7.1.0",
    "futoin-hkdf": "^1.3.3",
    "prompts": "^2.4.0"
  }
}
